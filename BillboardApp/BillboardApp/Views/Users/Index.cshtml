@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityUser>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            User Name
            @*@Html.DisplayNameFor(model => model.)*@
        </th>
        <th>
            Email
            @*@Html.DisplayNameFor(model => model.Score)*@
        </th>
        <th>
            Roles
            
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.ActionLink("Add", "AddRole", new { UserName = item.UserName }) |
                @Html.ActionLink("Get", "GetRoles", new { UserName = item.UserName }) |
                @Html.ActionLink("Delete", "DeleteRole", new { UserName = item.UserName })
            </td>
        </tr>
    }

</table>